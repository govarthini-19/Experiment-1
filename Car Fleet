class Solution:
    def carFleet(self, target: int, position: List[int], speed: List[int]) -> int:
        car=sorted(zip(position,speed),reverse=True)
        f=0
        prev_time=0
        for position,speed in car:
            time=(target-position)/speed
            if time>prev_time:
                f+=1
                prev_time=time
        return f
